            "reason": "no-compatible-stable-release"
        },
        "sunnysideup/vardump": {
            "section": "require",
            "current": "dev-master",
            "reason": "no-compatible-stable-release"
        },
        "sunnysideup/model-admin-many-tabs": {
            "section": "require",
