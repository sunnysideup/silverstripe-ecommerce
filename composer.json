{
    "name": "sunnysideup/ecommerce",
    "type": "silverstripe-vendormodule",
    "description": "Silverstripe E-commerce Application",
    "keywords": [
        "Silverstripe-CMS",
        "CMS",
        "Silverstripe",
        "e-commerce",
        "ecommerce",
        "payment",
        "shop",
        "sales",
        "webshop",
        "cart",
        "sales",
        "products",
        "selling stuff",
        "online payment",
        "services"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicolaas Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "require": {
        "silverstripe/cms": "^6.0",
        "bummzack/sortablefile": "^3.0",
        "pelago/emogrifier": "^8.0@stable",
        "silverstripe/lumberjack": "^1.1"
    },
    "suggest": {
        "sunnysideup/flush": "Better, faster output for tasks",
        "silverstripe/geoip": "Native Silverstripe way to work out the customer country. Make sure this works on your server, before installing it - otherwise it is useless.",
        "symbiote/silverstripe-grouped-cms-menu": "HIGHLY RECOMMENDED! merge e-commerce cms menu items into one section."
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Sunnysideup\\Ecommerce\\": "src/",
            "Sunnysideup\\Ecommerce\\Tests\\": "tests/"
        }
    },
    "yet-to-update": {
        "symbiote/silverstripe-grouped-cms-menu": {
            "section": "require",
            "current": "^4.0@stable",
            "reason": "no-compatible-stable-release"
        },
        "sunnysideup/google_address_field": {
            "section": "require",
            "current": "dev-master",
            "reason": "no-compatible-stable-release"
        },
        "sunnysideup/phone_field": {
            "section": "require",
            "current": "dev-master",
            "reason": "no-compatible-stable-release"
        },
        "sunnysideup/cms_edit_link_field": {
            "section": "require",
